<?xml version="1.0" encoding="UTF-8"?>
<plugin id="lycwed-cordova-plugin-admob-inmobi"
    version="1.0.3"
    xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <name>lycwed-cordova-plugin-admob-inmobi</name>
    <description>Cordova AdMob Mediation Plugin for InMobi</description>    
    <author>Lycwed</author>
    <keywords>admob,mediation,inmobi</keywords>
    <license>MIT</license>

    <engines>
        <engine name="cordova" version=">=3.0.0" />
    </engines>

    <platform name="android">
        <source-file src="src/android/libadapterinmobi.jar" target-dir="libs" />
    </platform>

    <platform name="ios">
        <header-file src="src/ios/GADInMobiExtras.h" />
        <header-file src="src/ios/GADMAdapterInMobi.h" />
        <source-file src="src/ios/libAdapterInMobi.a" framework="true" />
    </platform>
</plugin>
