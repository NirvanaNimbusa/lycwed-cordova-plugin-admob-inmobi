<?xml version="1.0" encoding="UTF-8"?>
<plugin id="lycwed-cordova-plugin-admob-inmobi" version="1.0.6"
    xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <name>lycwed-cordova-plugin-admob-inmobi</name>
    <description>Cordova AdMob Mediation Plugin for InMobi</description>

    <engines>
        <engine name="cordova" version=">=3.0.0" />
    </engines>

    <platform name="android">
        <source-file src="src/android/libadapterinmobi.jar" target-dir="libs" />
    </platform>

    <platform name="ios">
        <pods-config ios-min-version="9.0" use-frameworks="true" />
        <pod id="GoogleMobileAdsMediationInMobi" version="7.0.4.0" />
    </platform>
</plugin>
